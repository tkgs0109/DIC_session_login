<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path) do |f| %>
<table>
  <tr>
    <td><%= f.label :email %></td>
    <td><%= f.email_field :email %></td>
  </tr>
  <tr>
    <td><%= f.label :password %></td>
    <td><%= f.password_field :password %></td>
  </tr>
</table>
  <%= f.submit "Log in" %>
<% end %>